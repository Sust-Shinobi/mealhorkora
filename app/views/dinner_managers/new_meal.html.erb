<div class="row">
  <div class="col-md-5">
    <%= form_for(:dinner_meal,url: create_new_dinner_menu_path) do |f| %>
      <div class="col-md-4">
        <%= f.label :items %>
        <%= f.text_field :items, class: 'form-control'%>
        <br/>
        <%= f.label :cost %>
        <%= f.number_field :cost, class: 'form-control' %>
        <span class="picture">
        <%= f.file_field :picture %>
        </span>
        <%= f.submit "Add dinner", disable_with: 'Submiting...', action: "create_meal", class: "btn btn-danger" %>
      </div>
    <% end %>
  </div>
  <div class="col-md-7">
    <h3>hi</h3>
  </div>
</div>